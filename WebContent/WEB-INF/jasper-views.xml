<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="collections" class="org.springframework.web.servlet.view.jasperreports.JasperReportsMultiFormatView">
		<property name="url" value="classpath:resources/collection.jrxml"/>
	</bean>

	<bean id="pooja_slip" class="org.springframework.web.servlet.view.jasperreports.JasperReportsMultiFormatView">
		<property name="url" value="classpath:resources/pooja_slip.jrxml"/>
	</bean>

	<bean id="ticket" class="org.springframework.web.servlet.view.jasperreports.JasperReportsMultiFormatView">
		<property name="url" value="classpath:resources/ticket.jrxml"/>
	</bean>

</beans>
